<!DOCTYPE html>

    <%- include('../partials/head'); %>

    <body class="container">
        <%- include('../partials/header'); %>
        <main>
            <div class="jumbotron text-center mb-3">
                <h1>Node com EJS</h1>
                <p>Aprendendo a trabalhar com EJS.</p>
                <h4> <%= subtitle %></h4>
            </div>

            <ul id="home-list" class="jumbotron text-center">
                <% qualitys.forEach(function(quality){ %>
                    <li>
                        <strong><%= quality.title %></strong> 
                        <%= quality.message %> 
                    </li>
                <% }) %>
                
            </ul>

        </main>
        <%- include('../partials/footer'); %>
    </body>
</html>